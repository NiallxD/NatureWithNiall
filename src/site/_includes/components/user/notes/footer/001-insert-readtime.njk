<script>
    window.onload = function() {
        // Create a new paragraph element
        const p = document.createElement("p");
        p.id = "reading-time";

        // Find the h1 element
        const h1 = document.querySelector("h1");

        // Insert the new paragraph after the h1
        h1.parentNode.insertBefore(p, h1.nextSibling);
    }
</script>